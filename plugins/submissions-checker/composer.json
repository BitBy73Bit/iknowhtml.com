{
  "name": "bitby73bit/craft-submissions-checker",
  "description": "Submissions checker",
  "authors": [
    {
      "name": "bitby73bit",
      "homepage": "https://bitbytebit.co.uk"
    }
  ],
  "support": {
    "email": "mike@bitbytebit.co.uk"
  },
  "type": "craft-plugin",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.2.0",
    "ipinfo/ipinfo": "^3.1"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "bitby73bit\\craftsubmissionschecker\\": "src/"
    }
  },
  "extra": {
    "handle": "_submissions-checker",
    "name": "Submissions checker",
    "developer": "bitby73bit",
    "documentationUrl": ""
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}